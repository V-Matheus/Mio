    "test:watch": "vitest",
    "test:ui": "vitest --ui --coverage"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.2.0",
    "next": "16.1.5",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.2.0"
  },
  "devDependencies": {
    "@mio/testing-config": "*",
    "@mio/typescript-config": "*",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "typescript": "5.9.2"
  }
